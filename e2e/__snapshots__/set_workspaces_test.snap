[TestSetWorkspaces/set_workspaces_with_multiple_workspaces - 1]
sway-setter workspaces --print <<EOF
[
  {
    "id": 1,
    "name": "1",
    "output": "HDMI-A-0",
    "focused": false
  },
  {
    "id": 2,
    "name": "2",
    "output": "DP-1",
    "focused": true
  },
  {
    "id": 3,
    "name": "3",
    "output": "eDP-1",
    "focused": false
  }
]
EOF
workspace 1; move workspace to output HDMI-A-0
workspace 2; move workspace to output DP-1
workspace 3; move workspace to output eDP-1
workspace 2

