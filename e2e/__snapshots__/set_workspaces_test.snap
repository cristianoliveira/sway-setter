[TestSetWorkspaces/set_workspaces_one_workspace - 1]
sway-setter workspaces --print <<EOF
[
  {
    "id": 1,
    "name": "1",
    "output": "HDMI-A-0",
    "focused": true
  }
]
EOF
workspace 1; move workspace to output HDMI-A-0
workspace 1

